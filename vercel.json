{
  "version": 2,
  "public": true,
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": "",
  "devCommand": null,
  "installCommand": null,
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 